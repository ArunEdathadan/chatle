{
    "version": "1.0.0-*",
    "authors": [ "Olivier Lefebvre" ],
    "description": "ChatLe repository implementation for Identity framework",
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [ "DEBUG", "TRACE" ]
            }
        },
        "Release": {
            "compilationOptions": {
